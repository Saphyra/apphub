<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <changeSet id="create-table-universe" author="saphyra">
        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists schemaName="skyxplore_game" tableName="production_order"/>
            </not>
        </preConditions>

        <createTable schemaName="skyxplore_game" tableName="production_order">
            <column name="production_order_id" type="varchar(37)">
                <constraints nullable="false"/>
            </column>
            <column name="game_id" type="varchar(37)"/>
            <column name="location" type="varchar(37)"/>
            <column name="location_type" type="varchar(255)"/>
            <column name="assignee" type="varchar(37)"/>
            <column name="external_reference" type="varchar(37)"/>
            <column name="data_id" type="varchar(255)"/>
            <column name="amount" type="integer"/>
            <column name="required_work_points" type="integer"/>
            <column name="current_work_points" type="integer"/>
        </createTable>

        <addPrimaryKey schemaName="skyxplore_game" tableName="production_order" columnNames="production_order_id" constraintName="production_order_pk"/>
    </changeSet>
</databaseChangeLog>