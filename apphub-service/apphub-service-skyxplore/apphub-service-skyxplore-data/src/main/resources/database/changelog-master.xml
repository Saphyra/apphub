<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet id="create_schema_notebook" author="saphyra">
        <sql endDelimiter=";">
            CREATE SCHEMA IF NOT EXISTS notebook
        </sql>

        <rollback>
            DROP SCHEMA IF EXIST notebook CASCADE
        </rollback>
    </changeSet>

    <include file="database/change_sets/character.xml"/>
    <include file="database/change_sets/friend_request.xml"/>
    <include file="database/change_sets/friendship.xml"/>
</databaseChangeLog>